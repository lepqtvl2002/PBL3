@model IEnumerable<PBL3_Tutor.Models.Registration>
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="container-fluid">
    <!-- row -->
    <div class="row">
        <!--col -->
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
            <div class="white-box">
                <div class="col-in row">
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <i data-icon="E"
                           class="linea-icon linea-basic"></i>
                        <h5 class="text-muted vb">Khách hàng đăng ký thuê gia sư mới</h5>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <h3 class="counter text-right m-t-15 text-danger">@Session["Customers"]</h3>
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="progress">
                            <div class="progress-bar progress-bar-danger" role="progressbar"
                                 aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                                <span class="sr-only">40% Complete (success)</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col -->
        <!--col -->
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
            <div class="white-box">
                <div class="col-in row">
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <i class="linea-icon linea-basic"
                           data-icon="&#xe01b;"></i>
                        <h5 class="text-muted vb">Lớp mới chưa có gia sư</h5>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <h3 class="counter text-right m-t-15 text-megna">@Session["CLasses"]</h3>
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="progress">
                            <div class="progress-bar progress-bar-megna" role="progressbar"
                                 aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                                <span class="sr-only">40% Complete (success)</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col -->
        <!--col -->
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
            <div class="white-box">
                <div class="col-in row">
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <i class="linea-icon linea-basic"
                           data-icon="&#xe00b;"></i>
                        <h5 class="text-muted vb">Số lượng gia sư của hệ thống</h5>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <h3 class="counter text-right m-t-15 text-primary">@Session["Tutors"]</h3>
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="progress">
                            <div class="progress-bar progress-bar-primary" role="progressbar"
                                 aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                                <span class="sr-only">40% Complete (success)</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
    <!--row -->
    <div class="row">
        <div class="col-md-12">
            <div class="white-box">
                <h3 class="box-title">Biểu đồ</h3>
                <ul class="list-inline text-right">
                    <li>
                        <h5><i class="fa fa-circle m-r-5" style="color: #dadada;"></i>Số lượng gia sư</h5>
                    </li>
                    <li>
                        <h5><i class="fa fa-circle m-r-5" style="color: #aec9cb;"></i>Số lượng lớp học</h5>
                    </li>
                </ul>
                <div id="morris-area-chart2" style="height: 370px;"></div>
            </div>
        </div>
    </div>
    <!--row -->
    <div class="row">
        <div class="col-sm-12">
            <div class="white-box">
                <h3 class="box-title">
                    Các lớp mới có gia sư
                    <div class="col-md-2 col-sm-4 col-xs-12 pull-right">
                        <select class="form-control pull-right row b-none">
                            <option>March 2022</option>
                            <option>April 2022</option>
                            <option>May 2022</option>
                            <option>June 2022</option>
                            <option>July 2022</option>
                        </select>
                    </div>
                </h3>
                <div class="table-responsive">
                    <table class="table ">
                        <thead>
                            <tr>
                                <th>NAME</th>
                                <th>STATUS</th>
                                <th>CLASS_ID</th>
                                <th>FEE(%)</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                if (item.state == "Được chấp nhận")
                                {
                                <tr>
                                    <td class="txt-oflo">@item.Tutor.name</td>
                                    <td>@item.state</td>
                                    <td class="txt-oflo">@item.Class.classId</td>
                                    <td><span class="text-success">@item.Class.fee</span></td>
                                </tr>
                                }
                            }
                        </tbody>
                    </table> <a href="@Url.Action("Index","Registrations")">Check all the registers</a>
                </div>
            </div>
        </div>
    </div>
    <!-- /.row -->
    <!-- row -->
    <div class="row">
        <div class="col-md-12 col-lg-6 col-sm-12">
            <div class="white-box">
                <h3 class="box-title">Recent Comments</h3>
                <div class="comment-center">
                    <div class="comment-body">
